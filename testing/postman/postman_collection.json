    {
      "name": "Executions",
      "item": [
        {
          "name": "Get Execution Result",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 200', function() {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "const response = pm.response.json();",
                  "",
                  "pm.test('Execution has valid status', function() {",
                  "    const validStatuses = ['QUEUED', 'RUNNING', 'COMPLETED', 'FAILED', 'TIMEOUT'];",
                  "    pm.expect(validStatuses).to.include(response.status);",
                  "});",
                  "",
                  "if (response.status === 'COMPLETED') {",
                  "    pm.test('Completed execution has required fields', function() {",
                  "        pm.expect(response).to.have.property('stdout');",
                  "        pm.expect(response).to.have.property('stderr');",
                  "        pm.expect(response).to.have.property('execution_time_ms');",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
