      "item": [
        {
          "name": "Create Live Code Session",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('session_id', response.session_id);",
                  "    pm.test('Session created successfully', function() {",
                  "        pm.expect(response.session_id).to.be.a('string');",
                  "        pm.expect(response.status).to.equal('ACTIVE');",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"language\": \"python\",\n  \"source_code\": \"print('Hello World')\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/code-sessions",
              "host": ["{{base_url}}"],
              "path": ["code-sessions"]
            },
            "description": "Create a new live coding session with specified language and initial source code."
          },
          "response": [
            {
              "name": "Success Response",
              "originalRequest": {
                "method": "POST",
                "header": [],
